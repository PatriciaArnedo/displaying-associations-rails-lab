<h1>Songs</h1>

<ul>
  <% @songs.each do |song| %>
    <li>
      <a href="<%= song_path(song) %>">
        <%= song.artist_name %>
        -
        <%= song.title %>
      </a>
    </li>
  <% end %>
</ul>

<br>
<br>
<%= link_to "Add Song", new_song_path %>
<br>
<br>
<%= link_to "All Artists", artists_path %>
